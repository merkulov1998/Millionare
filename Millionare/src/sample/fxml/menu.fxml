<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="207.0" prefWidth="544.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Menu">
   <children>
      <Button id="btn_newPlay" fx:id="btn_newPlay" layoutX="33.0" layoutY="29.0" mnemonicParsing="false" onAction="#handleButtonClicks" prefHeight="150.0" prefWidth="150.0" text="Новая игра" />
      <Button id="btn_newQuestion" fx:id="btn_newQuestion" layoutX="206.0" layoutY="29.0" mnemonicParsing="false" onAction="#handleButtonClicks" prefHeight="150.0" prefWidth="150.0" text="Новый вопрос" textAlignment="JUSTIFY" textOverrun="LEADING_WORD_ELLIPSIS" />
      <Button id="btn_record" fx:id="btn_record" layoutX="373.0" layoutY="29.0" mnemonicParsing="false" onAction="#handleButtonClicks" prefHeight="150.0" prefWidth="150.0" text="Счет" textAlignment="JUSTIFY" textOverrun="LEADING_WORD_ELLIPSIS" />
   </children>
   <effect>
      <Lighting>
         <bumpInput>
            <Shadow color="#fffefe" />
         </bumpInput>
         <light>
            <Light.Distant color="#fff9f9" />
         </light>
      </Lighting>
   </effect>
</AnchorPane>
